@functions
        {
    public int Age(DateTime birthday)
    {
        DateTime now = DateTime.Today;

        int age = now.Year - birthday.Year;
        if (birthday > now.AddYears(-age)) age--;
        return age;
    }
}
@functions{
    public string GenerateHtmlString(object item)
    {
        if (item is string strValue && !string.IsNullOrEmpty(strValue))
        {
            return $"<p>{strValue}</p>";
        }
        else if (item is IEnumerable<object> objectEnumerableValue)
        {
            var stringBuilder = new StringBuilder();
            string currentUrl = Request.Url.AbsoluteUri;
            var urlitems = currentUrl.Split('/');
            foreach (var nestedItem in objectEnumerableValue)
            {
                //if (nestedItem is IEnumerable<object> objectValue)
                //{
                //    foreach (var Item in objectValue)
                //    {
                //        stringBuilder.Append(GenerateHtmlString(Item));
                //    }
                //}
                if (nestedItem.ToString().Count(c => c == '/') == 1)
                {
                    var Replacepart = urlitems[4] + '/' + urlitems[5];
                    var newUrl = currentUrl.Replace(Replacepart, nestedItem.ToString());
                    //return $"<p><a href={newUrl}>{nestedItem}</a></p>";
                    stringBuilder.Append($"<p><a href={newUrl}>{nestedItem}</a></p>");
                }
                else
                {
                    stringBuilder.Append(GenerateHtmlString(nestedItem)); // 递归调用，处理嵌套数据结构
                }
            }
            return stringBuilder.ToString();
        }
        else
        {
            return string.Empty;
        }
    }
}
